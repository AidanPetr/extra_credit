https://www.hackerrank.com/challenges/swap-case/problem
def swap_case(s):
    
    Output="";
    for letter in s:
        if(letter.isupper()==True):
            Output += (letter.lower());
        elif(letter.islower()==True):
            Output += (letter.upper());
        else:
            Output += letter;
    return Output

if __name__ == '__main__':
    s = input()
    result = swap_case(s)
    print(result)
